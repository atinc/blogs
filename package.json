{
  "name": "worktile",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "npm run dev:wt",
    "dev:wt": "webpack --config webpack.config.js --mode development --watch --env.project=worktile",
    "build:wt": "webpack --config webpack.config.js --mode production --env.project=worktile",
    "dev:pc": "webpack --config webpack.config.js --mode development --watch --env.project=pingcode",
    "build:pc": "webpack --config webpack.config.js --mode production --env.project=pingcode",
    "docker-init": "docker-compose up",
    "compress:pc": "node compress-themes pc",
    "compress:wt": "node compress-themes wt",
    "rename-header:wt": "mv ./src/worktile/pages/partials/header.html ./src/worktile/pages/partials/header.hbs",
    "rename-footer:wt": "mv ./src/worktile/pages/partials/footer.html ./src/worktile/pages/partials/footer.hbs",
    "copy:wt": "cpx './node_modules/@atinc/dione-worktile/*.html' './src/worktile/pages/partials/'",
    "dione:wt": "npm run copy:wt && npm run rename-header:wt && npm run rename-footer:wt",
    "rename-header:pc": "mv ./src/pingcode/pages/partials/header.html ./src/pingcode/pages/partials/header.hbs",
    "rename-footer:pc": "mv ./src/pingcode/pages/partials/footer.html ./src/pingcode/pages/partials/footer.hbs",
    "copy:pc": "cpx './node_modules/@atinc/dione-pingcode/*.html' './src/pingcode/pages/partials/'",
    "dione:pc": "npm run copy:pc && npm run rename-header:pc && npm run rename-footer:pc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@atinc/dione-worktile": "^1.0.10",
    "@atinc/dione-pingcode": "^1.0.10",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@worktile/pc-site-styx": "latest",
    "babel-loader": "^8.0.0-beta.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^1.3.0",
    "lodash": "^4.17.20",
    "node-sass": "^4.13.0",
    "sass": "^1.23.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/jquery": "^3.5.3",
    "compressing": "^1.5.1",
    "jquery": "^3.4.1",
    "uikit": "^3.2.2"
  }
}
